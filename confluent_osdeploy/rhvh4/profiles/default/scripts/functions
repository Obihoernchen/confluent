run_remote() {
    cd $(mktemp -d)
    curl -f https://$mgr/confluent-public/os/$profile/scripts/$1 > $1
    chmod +x $1
    ./$1
    cd -
}

run_remote_python() {
    cd $(mktemp -d)
    curl -f https://$mgr/confluent-public/os/$profile/scripts/$1 > $1
    /usr/libexec/platform-python $1
    cd -
}
